{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": ["dockerfile"],
      "enabled": false
    },
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": [
        "docker.io/library/postgres",
        "postgres",
        "docker.io/library/redis",
        "redis"
      ],
      "enabled": false
    }
  ]
}
